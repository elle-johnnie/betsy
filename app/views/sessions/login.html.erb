<main class="main-container">
  <h2>Log In</h2>

  <%= form_tag do %>
    <div class="form-group">
      <%= label_tag :username, "Username" %>
      <%= text_field_tag :username, nil, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= submit_tag "Log In", class: "btn btn-primary" %>
    </div>
  <% end %>
</main>
